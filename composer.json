{
  "name": "orchard/test_utils",
  "type": "utility",
  "description": "Orchard Test Utils",
  "keywords": ["utils"],
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:theorchard/common.git"
    }
  ],
  "require": {
    "orchard/common": "2.1.4",
    "silex/silex": "1.2.3",
    "guzzlehttp/guzzle": "~6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.2.6",
    "pdepend/pdepend": "1.1.1",
    "phpmd/phpmd": "1.5.0",
    "sebastian/phpcpd": "2.0.0",
    "phploc/phploc": "2.0.3",
    "mayflower/php-codebrowser": "1.1.1",
    "squizlabs/php_codesniffer": "2.3.0",
    "jakub-onderka/php-parallel-lint": "0.8"
  },
  "autoload": {
    "psr-4": {
      "Orchard\\CiUtils\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Orchard\\CiUtils\\Tests\\": "tests/unit/"
    }
  },
  "bin": [
    "scripts/process_payload"
  ]
}
